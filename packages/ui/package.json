{
    "name": "@truststack/ui",
    "version": "0.5.1",
    "description": "Trust Stack UI library",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "react-native": {
                "import": "./dist/esm/index.native.js",
                "require": "./dist/cjs/index.native.js"
            },
            "types": "./types/index.d.ts",
            "import": "./dist/esm/index.mjs",
            "require": "./dist/cjs/index.cjs",
            "default": "./dist/cjs/index.native.js"
        },
        "./react-native-web": {
            "react-native": "./dist/cjs/react-native-web.native.js",
            "types": "./types/index.d.ts",
            "import": "./dist/esm/react-native-web.mjs",
            "require": "./dist/cjs/react-native-web.cjs"
        },
        "./web": {
            "react-native": {
                "import": "./dist/esm/index.native.js",
                "require": "./dist/cjs/index.native.js"
            },
            "types": "./types/index.d.ts",
            "import": "./dist/esm/index.mjs",
            "require": "./dist/cjs/index.cjs"
        },
        "./native": {
            "types": "./types/index.d.ts",
            "require": "./dist/native.js",
            "react-native": "./dist/native.native.js"
        },
        "./native-test": {
            "types": "./types/index.d.ts",
            "require": "./dist/test.cjs",
            "react-native": "./dist/test.native.js"
        }
    },
    "main": "dist/cjs",
    "module": "dist/esm",
    "source": "src/index.tsx",
    "types": "./types/index.d.ts",
    "scripts": {
        "build": "tamagui-build",
        "clean": "tamagui-build clean",
        "watch": "tamagui-build --watch"
    },
    "dependencies": {
        "@tamagui/shorthands": "*",
        "@tamagui/themes": "*",
        "@tamagui/core": "*",
        "@tamagui/animations-react-native": "*",
        "@datepicker-react/hooks": "^2.8.4",
        "@floating-ui/react": "0.27.4",
        "@truststack/icons-ui": "workspace:*",
        "@tanstack/react-table": "^8.16.0",
        "change-case": "^4.1.2",
        "date-fns": "^2.28.0",
        "html5-qrcode": "^2.3.8",
        "react": "*",
        "react-dom": "*",
        "react-native": "*",
        "react-native-svg": "15.2.0",
        "react-native-web": "~0.19.6",
        "react-native-webview": "^13.13.2",
        "tamagui": "*"
    },
    "devDependencies": {
        "@storybook/react": "^8.6.12",
        "@tamagui/build": "*",
        "@types/react": "*"
    },
    "publishConfig": {
        "access": "public"
    },
    "removeSideEffects": "true",
    "tamagui": {
        "build": {
            "skipEnvToMeta": true,
            "bundle.native": "./src/index.ts",
            "bundle.native.test": "./src/index.ts"
        }
    }
}