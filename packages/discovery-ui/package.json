{
  "name": "@truststack/discovery-ui",
  "version": "0.4.0",
  "description": "Trust Stack Discovery UI library",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native": {
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./react-native-web": {
      "react-native": "./dist/cjs/react-native-web.native.js",
      "types": "./types/index.d.ts",
      "import": "./dist/esm/react-native-web.mjs",
      "require": "./dist/cjs/react-native-web.cjs"
    },
    "./web": {
      "react-native": {
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./native": {
      "types": "./types/index.d.ts",
      "require": "./dist/native.js",
      "react-native": "./dist/native.native.js"
    },
    "./native-test": {
      "types": "./types/index.d.ts",
      "require": "./dist/test.cjs",
      "react-native": "./dist/test.native.js"
    }
  },
  "main": "dist/cjs",
  "module": "dist/esm",
  "source": "src/index.tsx",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "tamagui-build",
    "clean": "tamagui-build clean",
    "watch": "tamagui-build --watch"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@tamagui/core": "*",
    "@tamagui/web": "*",
    "@tanstack/react-table": "^8.16.0",
    "@truststack/credential-ui": "workspace:*",
    "@truststack/icons-ui": "workspace:*",
    "@truststack/nav-ui": "workspace:*",
    "@truststack/render-ui": "workspace:*",
    "@truststack/schema": "workspace:*",
    "@truststack/theme-ui": "workspace:*",
    "@truststack/ui": "workspace:*",
    "d3-force": "^3.0.0",
    "elkjs": "^0.10.0",
    "react-flow-renderer": "^10.3.17",
    "tamagui": "*"
  },
  "devDependencies": {
    "@storybook/react": "^8.6.12",
    "@tamagui/build": "*",
    "@types/d3-force": "^3.0.10",
    "@types/react": "*",
    "@types/react-dom": "*"
  },
  "removeSideEffects": "true",
  "tamagui": {
    "build": {
      "skipEnvToMeta": true,
      "bundle.native": "./src/index.ts",
      "bundle.native.test": "./src/index.ts"
    }
  }
}